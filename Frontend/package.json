{
  "name": "gatsby-payroll-typescript-project",
  "private": true,
  "description": "Gatsby Payroll management system in typescript",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop:gatsby": "gatsby develop",
    "develop:type-check": "tsc --watch",
    "develop": "npm-run-all --parallel develop:gatsby develop:type-check",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest --watchAll",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@gatsbyjs/reach-router": "^2.0.1",
    "@reach/router": "^1.3.4",
    "@types/papaparse": "^5.3.7",
    "@types/react": "^18.2.6",
    "axios": "^1.4.0",
    "bootstrap": "^5.2.3",
    "canvas": "^2.11.2",
    "csv-parser": "^3.0.0",
    "gatsby": "^5.9.0",
    "gatsby-plugin-catch-links": "^5.9.0",
    "gatsby-source-filesystem": "^5.9.0",
    "gatsby-transformer-json": "^5.9.0",
    "gatsby-transformer-remark": "^6.9.0",
    "html2canvas": "^1.4.1",
    "install-link": "^0.1.2",
    "jest-css-modules-transform": "^4.4.2",
    "jspdf": "^2.5.1",
    "link-react": "^3.0.0",
    "material-react-toastify": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-test-renderer": "^18.2.0",
    "react-toastify": "^9.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.21.5",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.1",
    "@types/reach__router": "^1.3.11",
    "@types/react-modal": "^3.16.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react-toastify": "^4.1.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-preview": "^0.3.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es/.*)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^.+\\.(css|less)$": "<rootDir>/config/CSSStub.js"
    },
    "testEnvironment": "jsdom"
  }
}
